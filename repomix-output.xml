/* Import Poppins font */
@import url("https://fonts.googleapis.com/css2?family=Poppins:wght@400;600;900&display=swap");

/* Page Styles */
.domain-page {
  padding: 50px 20px;
  background-color: #001f3f; /* Dark blue background */
  min-height: 100vh;
  font-family: "Poppins", sans-serif;
  color: #e7eaee;
}

.domain-page-title {
  text-align: center;
  font-size: 3rem;
  font-weight: 900;
  margin-bottom: 40px;
}

/* Level Filters */
.level-filters {
  display: flex;
  justify-content: center;
  gap: 20px;
  margin-bottom: 40px;
}

.level-filter {
  padding: 10px 20px;
  font-size: 1.3rem;
  font-weight: 600;
  border: 2px solid #cc1e4a;
  border-radius: 5px;
  background-color: transparent;
  color: #e7eaee;
  cursor: pointer;
  transition: all 0.3s ease;
  width: auto; /* Ensure buttons only take up necessary width */
  min-width: 250px; /* Set a minimum width for consistency */
}

.level-filter.active,
.level-filter:hover {
  background-color: #cc1e4a;
  color: #e7eaee;
  transform: translateY(-2px);
}

/* Domain Cards */
.domains-container {
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  gap: 47px;
  padding: 20px;
}

.domain-card {
  position: relative;
  width: 350px;
  height: 400px;
  background-color: #e7eaee;
  border: 4px solid #cc1e4a;
  border-radius: 20px;
  padding: 20px;
  text-align: center;
  color: #001f3f;
  cursor: pointer;
  overflow: visible;
  transition: all 0.3s ease;
}

.domain-card:hover {
  transform: translateY(-10px);
  box-shadow: -12px 16px 7px 1px rgba(0, 0, 0, 0.6);
  background-color: #cc1e4a;
  color: #e7eaee;
}

.top-image {
  width: 160px;
  height: 160px;
  margin: -20px auto 30px;
  background-color: purple;
  border-radius: 10px;
  display: flex;
  align-items: center;
  justify-content: center;
}

.top-image img {
  width: 160px;
  height: auto;
}

.domain-title {
  font-size: 2rem;
  font-weight: 900;
  margin-top: 120px;
  transition: transform 0.3s ease;
  margin-bottom: 15px;
}

.domain-card:hover .domain-title {
  opacity: 0; /* Hide default title on hover */
}

.domain-description {
  font-size: 1rem;
  margin-bottom: 20px;
  transition: opacity 0.3s ease;
  text-align: center;
  margin-top: 10px;
  padding: 0 20px;
  width: 100%;
  box-sizing: border-box;
  display: block;
  margin-left: 0px;
}

.domain-card:hover .domain-description {
  opacity: 0; /* Hide description on hover */
}

/* Hover Content */
.hover-content {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.8);
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
  opacity: 0;
  transition: opacity 0.3s ease;
  padding-top: 20px;
  border-radius: 20px;
}

.domain-card:hover .hover-content {
  opacity: 1;
}

.hover-domain-title {
  font-size: 2rem;
  font-weight: 900;
  margin-bottom: 3px;
  color: #e7eaee;
}

.hover-topics {
  margin-bottom: 1px;
}

.hover-topics p {
  font-size: 1.2rem;
  margin: 5px 0;
}

.hover-levels {
  display: flex;
  gap: 10px;
  margin-top: 10px;
}

.hover-levels button {
  padding: 5px 10px;
  font-size: 0.9rem;
  border: 2px solid #e7eaee;
  border-radius: 5px;
  background-color: transparent;
  color: #e7eaee;
  cursor: pointer;
  transition: all 0.3s ease;
}

.hover-levels button:hover {
  background-color: #e7eaee;
  color: #cc1e4a;
}

/* Click Here Button */
/* Click Here Button - Change from full width to centered with specific width */
.click-button {
  position: absolute;
  bottom: 20px;
  left: 50%;
  transform: translateX(-50%);
  padding: 10px 20px;
  font-size: 1rem;
  font-weight: 600;
  border: 2px solid #001f3f;
  border-radius: 5px;
  background-color: #cc1e4a;
  color: #e7eaee;
  cursor: pointer;
  transition: all 0.3s ease;
  width: auto; /* Change from default (which stretches) to auto */
  min-width: 120px; /* Add a minimum width to ensure button isn't too small */
  max-width: 60%; /* Limit width to approximately 60% of the card */
}

.domain-card:hover .click-button {
  background-color: #e7eaee;
  color: #cc1e4a;
  bottom: 10px; /* Move the button down during hover */
}

/* Add Domain Card Styles */
.add-domain-card {
  background-color: #f0f0f0;
  border: 3px dashed #cc1e4a; /* Changed to dashed border for better visual */
  color: #333;
  cursor: pointer;
  overflow: hidden; /* Contain effects within card */
}

.add-domain-card .top-image {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 160px;
  background-color: #e0e0e0;
}

.add-domain-card .top-image img {
  width: 80px;
  height: 80px;
  opacity: 0.8;
}

.add-domain-card .domain-title {
  font-size: 2rem;
  color: #007bff;
  margin: 15px 0 10px;
  margin-top: 100px; /* Added to move text down */
}

.add-domain-card .domain-description {
  font-size: 1rem;
  color: #555;
  transition: all 0.3s ease; /* Add transition for smoother effect */
}

.add-domain-card:hover {
  background-color: #cc1e4a;
  transform: translateY(-10px) scale(1.02); /* Add slight scaling effect */
  box-shadow: 0 15px 25px rgba(0, 0, 0, 0.3); /* Improved shadow */
}

.add-domain-card:hover .domain-title,
.add-domain-card:hover .domain-description {
  opacity: 1; /* Changed from 0 to keep text visible */
  color: white; /* Change text color on hover */
  transform: translateY(20px); /* Add slight movement effect */
  font-weight: 600; /* Make text bolder on hover */
}

.add-domain-card:hover::after {
  content: "+";
  position: absolute;
  top: 40px;
  left: 50%;
  transform: translateX(-50%);
  font-size: 5rem;
  color: rgba(255, 255, 255, 0.7);
}

.add-domain-card .click-button {
  background-color: #28a745;
  color: white;
  font-weight: bold;
  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.2);
}

.add-domain-card:hover .click-button {
  background-color: #e7eaee;
  color: #cc1e4a;
}
