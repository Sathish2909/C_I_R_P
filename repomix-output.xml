}

  .social:hover svg path {
    fill: url(#paint0_linear); /* Apply the gradient fill on hover */
  } 


  #logo {
    fill: #013F59;
  }
  
  #logo:hover {
    fill: url(#paint0_linear);
  }
  
  .dev-btn {
    width: 100%;
    height: 30%;
    display: flex;
    text-align: center;
    align-items: center;
    justify-content: center;
  }
  
  .dev-btn button {
    width: 100px;
