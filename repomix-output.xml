      opacityIn: [0, 1],
      scaleIn: [0.2, 1],
      scaleOut: 3,
      durationIn: 800,
      durationOut: 600,
      delay: 500
    };

    anime.timeline({ loop: true })
      .add({
        targets: '.intro-1',
        opacity: heading.opacityIn,
        scale: heading.scaleIn,
        duration: heading.durationIn
      })
      .add({
        targets: '.intro-1',
        opacity: 0,
        scale: heading.scaleOut,
        duration: heading.durationOut,
        easing: "easeInExpo",
        delay: heading.delay
      })
      .add({
        targets: '.intro-2',
        opacity: heading.opacityIn,
        scale: heading.scaleIn,
        duration: heading.durationIn
      })
