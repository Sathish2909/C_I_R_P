/* Import Modern Google Font */
@import url("https://fonts.googleapis.com/css2?family=Poppins:wght@400;600;900&display=swap");

:root {
  --primary-color: #cc1e4a;
  --secondary-color: #e7eaee;
  --text-color: #e7eaee;
  --background-color: #001f3f;
  --card-bg: #0d2d50;
  --hover-bg: #cc1e4a;
}

/* Page Styles */
.project-page {
  font-family: "Poppins", sans-serif;
  background-color: var(--background-color);
  color: var(--text-color);
  min-height: 100vh;
  padding: 50px 10%;
  text-align: center;
}

/* Project Header */
.project-header {
  margin-bottom: 40px;
}

.project-header h1 {
  font-size: 3rem;
  font-weight: 900;
  color: var(--secondary-color);
}

.project-subtitle {
  font-size: 1.4rem;
  color: #cfcfcf;
  margin-top: 10px;
}

/* Project Content Layout */
.project-container {
  display: flex;
  justify-content: space-between;
  gap: 30px;
  align-items: flex-start;
}

/* Project Details */
.project-details {
  flex: 2;
  background-color: var(--card-bg);
  padding: 30px;
  border-radius: 15px;
  box-shadow: 0px 10px 20px rgba(0, 0, 0, 0.5);
  text-align: left;
}

.content-box {
  margin-bottom: 30px;
}

.content-box h2 {
  font-size: 1.8rem;
  color: var(--secondary-color);
  margin-bottom: 10px;
}

.content-box p {
  font-size: 1rem;
  color: #cfcfcf;
  line-height: 1.6;
}

/* Comments */
.comment-input {
  width: 100%;
  height: 100px;
  padding: 10px;
  border: 1px solid #ccc;
  border-radius: 6px;
  font-size: 1rem;
  color: #333;
}

.add-comment-button {
  background: var(--primary-color);
  color: white;
  padding: 10px 25px;
  border: none;
  cursor: pointer;
